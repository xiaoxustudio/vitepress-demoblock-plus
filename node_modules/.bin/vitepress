#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/f/ex/vitepress-demoblock-plus/node_modules/.pnpm/vitepress@1.3.4_@algolia+client-search@4.24.0_@types+node@18.19.46_async-validator@4.2.5_post_xw2mxio4ub4obaqnz77vnuhdzu/node_modules/vitepress/bin/node_modules:/mnt/f/ex/vitepress-demoblock-plus/node_modules/.pnpm/vitepress@1.3.4_@algolia+client-search@4.24.0_@types+node@18.19.46_async-validator@4.2.5_post_xw2mxio4ub4obaqnz77vnuhdzu/node_modules/vitepress/node_modules:/mnt/f/ex/vitepress-demoblock-plus/node_modules/.pnpm/vitepress@1.3.4_@algolia+client-search@4.24.0_@types+node@18.19.46_async-validator@4.2.5_post_xw2mxio4ub4obaqnz77vnuhdzu/node_modules:/mnt/f/ex/vitepress-demoblock-plus/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/f/ex/vitepress-demoblock-plus/node_modules/.pnpm/vitepress@1.3.4_@algolia+client-search@4.24.0_@types+node@18.19.46_async-validator@4.2.5_post_xw2mxio4ub4obaqnz77vnuhdzu/node_modules/vitepress/bin/node_modules:/mnt/f/ex/vitepress-demoblock-plus/node_modules/.pnpm/vitepress@1.3.4_@algolia+client-search@4.24.0_@types+node@18.19.46_async-validator@4.2.5_post_xw2mxio4ub4obaqnz77vnuhdzu/node_modules/vitepress/node_modules:/mnt/f/ex/vitepress-demoblock-plus/node_modules/.pnpm/vitepress@1.3.4_@algolia+client-search@4.24.0_@types+node@18.19.46_async-validator@4.2.5_post_xw2mxio4ub4obaqnz77vnuhdzu/node_modules:/mnt/f/ex/vitepress-demoblock-plus/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vitepress/bin/vitepress.js" "$@"
else
  exec node  "$basedir/../vitepress/bin/vitepress.js" "$@"
fi
